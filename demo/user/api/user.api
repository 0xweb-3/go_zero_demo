syntax = "v1"

info (
	title:   "用户服务api"
	desc:    "用户服务api"
	author:  "type author here"
	email:   "type email here"
	version: "type version here"
)

type (
	UserReq {
		Id string `json:"id"`
	}
	UserResp {
		Id    string `json:"id"`
		Name  string `json:"name"`
		Phone string `json:"phone"`
	}
)

// 定义user api服务描述
service User {
	@handler getUser
	get /user (UserReq) returns (UserResp)
}